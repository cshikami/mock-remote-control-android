<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shikamic.ShikamiC_remotecontrol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="shikamic.ShikamiC_remotecontrol.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="shikamic.ShikamiC_remotecontrol.MydvrActivity"
            android:label="MydvrActivity" >
        </activity>
        <activity android:name="shikamic.ShikamiC_remotecontrol.MyconfigureActivity"
            android:label="MyconfigureActivity" >
        </activity>
    </application>

</manifest>